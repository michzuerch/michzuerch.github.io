{
  "name": "@michzuerch/michzuerch.github.io",
  "description": "Homepage Michael Zuercher",
  "version": "0.1.1",
  "private": true,
  "homepage": "https://github.com/michzuerch/michzuerch.github.io#readme",
  "author": {
    "name": "Michael Zuercher",
    "email": "michzuerch@gmail.com",
    "url": "https://github.io/michzuerch"
  },
  "repository": "github:michzuerch/michzuerch.github.io",
  "keywords": [
    "Homepage",
    "Michael Zuercher",
    "michzuerch.github.io",
    "Rust"
  ],
  "displayName": "Homepage Michael Zuercher",
  "scripts": {
    "dev": "./node_modules/.bin/astro dev",
    "start": "./node_modules/.bin/astro dev",
    "build": "./node_modules/.bin/astro build",
    "clean": "./node_modules/.bin/rimraf dist .astro node_modules",
    "build:clean": "sudo rm -rf ./playwright-report/mobile-* && sudo rm -rf ./playwright-report/desktop-*",
    "build:docker": "podman build -t privaterautomarkt8-playwright:latest .",
    "test:docker": "podman run --name privaterautomarkt8-playwright --rm -v $(pwd)/playwright-report:/app/playwright-report -p 9323:9323 --network host privaterautomarkt8-playwright:latest",
    "test": "LOCAL_TEST=1 npx playwright test",
    "test:snapshots": "LOCAL_TEST=1 npx playwright test --update-snapshots",
    "test:lighthouse": "npx lighthouse http://localhost:3000/ --output html --output-path lighthouse-result.html view",
    "preview": "./node_modules/.bin/astro preview",
    "astro": "./node_modules/.bin/astro",
    "format": "./node_modules/.bin/prettier -w .",
    "lint:eslint": "./node_modules/.bin/eslint . --ext .js,.ts,.astro",
    "subfont": "./node_modules/.bin/subfont -ir --no-fallbacks --silent --root dist",
    "updates": "./node_modules/.bin/npm-check-updates",
    "upgrade": "./node_modules/.bin/npm-check-updates -u"
  },
  "devDependencies": {
    "@akebifiky/remark-simple-plantuml": "^1.0.2",
    "@astrojs/alpinejs": "^0.3.1",
    "@astrojs/image": "^0.17.0",
    "@astrojs/mdx": "^2.0.0",
    "@astrojs/sitemap": "^3.0.3",
    "@astrojs/tailwind": "^5.0.3",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@tailwindcss/typography": "^0.5.10",
    "@types/alpinejs": "^3.13.5",
    "@types/leaflet": "^1.9.8",
    "@types/photoswipe": "^4.1.6",
    "alpinejs": "^3.13.3",
    "astro": "^4.0.3",
    "astro-robots-txt": "^1.0.0",
    "eslint": "^8.55.0",
    "exifr": "^7.1.3",
    "mdast-util-to-string": "^4.0.0",
    "playwright": "^1.40.1",
    "prettier": "3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "reading-time": "^1.5.0",
    "rehype-katex": "^7.0.0",
    "remark-math": "^6.0.0",
    "rimraf": "^5.0.5",
    "schema-dts": "^1.1.2",
    "tailwindcss": "^3.3.6",
    "unist-util-visit": "^5.0.0",
    "npm-check-updates": "^16.14.11"
  },
  "dependencies": {
    "@astrojs/rss": "^4.0.1",
    "astro-icon": "^0.8.2",
    "astro-seo": "^0.8.0",
    "hero-patterns": "^2.1.0",
    "katex": "^0.16.9",
    "leaflet": "^1.9.4",
    "lunr": "^2.3.9",
    "photoswipe": "^5.4.3",
    "photoswipe-dynamic-caption-plugin": "^1.2.7",
    "swiper": "^11.0.5"
  }
}
