{
  "name": "michzuerch.github.io",
  "version": "1.0.0",
  "description": "Personal webpage github",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michzuerch/michzuerch.github.io.git"
  },
  "author": "Michael Zuercher",
  "main": "postcss.config.js",
  "dependencies": {
      "autoprefixer": "^9.6.0",
      "postcss-cli": "^6.1.2",
      "rimraf": "^2.6.3"
  },
  "devDependencies": {
      "@fullhuman/postcss-purgecss": "^1.2.0",
      "cssnano": "^4.1.10",
      "tailwindcss": "^1.0.4"
  },
  "scripts": {
      "del:js": "rimraf tailwind.config.js",
      "create:js": "./node_modules/.bin/tailwind init tailwind.config.js",
      "dev:css": "./node_modules/.bin/tailwind build tailwind.config.css -c tailwind.config.js -o dist/css/tailwind.css",
      "build:css": "postcss tailwind.config.css -o dist/css/tailwind.min.css"
  }
}
