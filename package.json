    "exifr": "^7.1.3",
    "mdast-util-to-string": "^3.2.0",
    "playwright": "^1.35.0",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "reading-time": "^1.5.0",
