{
  "name": "michzuerch.github.io",
  "version": "1.0.0",
  "description": "Personal webpage github",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michzuerch/michzuerch.github.io.git"
  },
  "author": "Michael Zuercher",
  "main": "postcss.config.js",
  "scripts": {
    "serve": "cross-env NODE_ENV=development concurrently \"postcss public/tailwind.css -o public/build/tailwind.css --watch\"  \"live-server ./public\"",
    "development": "cross-env NODE_ENV=development postcss public/tailwind.css -o public/build/tailwind.css",
    "production": "cross-env NODE_ENV=production postcss public/tailwind.css -o public/build/tailwind.css"
  },
  "dependencies": {
    "autoprefixer": "^9.6.0",
    "tailwindcss": "^1.0.4",
    "tailwindcss-typography": "^2.1.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "cssnano": "^4.1.10",
    "live-server": "^1.2.1",
    "postcss-cli": "^6.1.2"
  }
}
